{
    "version": 2,
    "name": "db",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],
    "functions": {
        "server.js": {
            "memory": 1024,
            "includeFiles": "db.json"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "server.js"
        }
    ]
}